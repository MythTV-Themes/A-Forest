        </fontdef>

        <fontdef name="basegothicmedium" face="League Gothic">
            <pixelsize>55</pixelsize>
            <color>#FFFFFF</color>
        </fontdef>

        <fontdef name="basegothicsmall" face="League Gothic">
            <pixelsize>25</pixelsize>
            <color>#FFFFFF</color>
        </fontdef>

        <fontdef name="basegothicsmalllightblue" from="basegothicsmall">
            <color>#0767AB</color>
        </fontdef>

        <fontdef name="basegothicmidblue" from="basegothicsmalllightblue">
            <color>#3A9ADE</color>
            <pixelsize>38</pixelsize>
        </fontdef>

        <shape name="rightpanel_dark">
            <area>1238,0,682,1080</area>
            <fill color="#000000" alpha="150" />
        </shape>

        <imagetype name="rightpanel_bg">
            <position>1100,0</position>
            <filename>images/editmetadata_leftpanelwide_bg.png</filename>
        </imagetype>

        <shape name="infobox">
            <area>1250,210,660,580</area>
            <type>roundbox</type>
            <fill color="#000000" alpha="180" />
        </shape>

        <textarea name="tit">
            <area>1260,234,674,60</area>
            <font>basegothicmedium</font>
            <align>left,vcenter</align>
            <value>Channel Editor</value>
        </textarea>

        <textarea name="message">
            <area>1260,285,674,57</area>
            <multiline>yes</multiline>
            <font>basesmalllightblue</font>
            <align>left,vcenter</align>
            <cutdown>yes</cutdown>
            <value>Edit the details about this channel.</value>
        </textarea>

        <textarea name="callsign_label">
            <area>1140,392,250,30</area>
            <align>right,vcenter</align>
            <font>basesmall</font>
            <value>Callsign:</value>
        </textarea>

        <textedit name="callsign">
            <area>1396,384,500,38</area>
            <statetype name="background">
                <state name="active">
                    <imagetype name="background">
                        <filename>images/textedit_unselected.png</filename>
                    </imagetype>
                </state>
                <state name="selected">
                    <imagetype name="background">
                        <filename>images/textedit_selected.png</filename>
                    </imagetype>
                </state>
                <state name="inactive">
                    <imagetype name="background">
                        <filename>images/textedit_unselected.png</filename>
                    </imagetype>
                </state>
            </statetype>
            <imagetype name="cursor">
                <position>0,0</position>
                <filename>images/textedit_cursor.png</filename>
            </imagetype>
            <textarea name="text">
                <area>5,8,490,30</area>
                <font>basesmall</font>
                <align>allcenter</align>
            </textarea>
        </textedit>

        <textarea name="channum_label">
            <area>1140,452,250,30</area>
            <align>right,vcenter</align>
            <font>basesmall</font>
            <value>Channel #:</value>
        </textarea>

        <textedit name="channum" from="callsign">
            <position>1396,444</position>
        </textedit>

        <textarea name="channame_label">
            <area>1140,512,250,30</area>
            <align>right,vcenter</align>
            <font>basesmall</font>
            <value>Chan Name:</value>
        </textarea>

        <textedit name="channame" from="callsign">
            <position>1396,504</position>
        </textedit>

        <textarea name="xmltv_label">
            <area>1140,572,250,30</area>
            <align>right,vcenter</align>
            <font>basesmall</font>
            <value>XMLTV #:</value>
        </textarea>

        <textedit name="XMLTV" from="callsign">
            <position>1396,564</position>
        </textedit>

        <button name="probe">
            <position>1396,694</position>
            <value>Probe</value>
            <statetype name="buttonstate">
                <state name="active">
                    <imagetype name="buttonbackground">
                        <area>0,0,156,38</area>
                        <filename>images/spinbox_unselected.png</filename>
                    </imagetype>
                    <textarea name="text">
                        <area>5,8,146,30</area>
                        <align>allcenter</align>
                        <font>basesmall</font>
                        <alpha>100</alpha>
                    </textarea>
                </state>
                <state name="selected" from="active">
                    <imagetype name="buttonbackground">
                        <area>0,0,156,38</area>
                        <filename>images/spinbox_selected.png</filename>
                    </imagetype>
                    <textarea name="text">
                        <alpha>255</alpha>
                    </textarea>
                </state>
                <state name="disabled" from="active" />
                <state name="pushed" from="selected">
                </state>
            </statetype>
        </button>

        <button name="ok" from="probe">
            <position>1594,694</position>
            <value>OK</value>
        </button>

    </window>

<!-- menu -->
    <window name="MythPopupBox">
        <area>0,0,1280,720</area>
        <shape name="shape_popup_background">
            <area>415,60,450,600</area>
            <type>roundbox</type>
            <fill color="#000000" alpha="150" />
            <line color="#999999" alpha="250" width="1" />
            <cornerradius>2</cornerradius>
        </shape>

        <shape name="shape_title_base">
            <area>425,80,430,80</area>
            <type>roundbox</type>
            <fill style="gradient">
                <gradient direction="diagonal" alpha="255">
                    <stop position="0" color="#202020" />
                    <stop position="45" color="#262626" />
                    <stop position="55" color="#303030" />
                    <stop position="100" color="#7f7f7f" />
                </gradient>
            </fill>
            <line color="#FFFFFF" alpha="255" width="1" />
            <cornerradius>4</cornerradius>
        </shape>

        <textarea name="messagearea">
            <area>425,80,430,80</area>
            <font>normal</font>
            <multiline>yes</multiline>
            <align>allcenter</align>
            <cutdown>yes</cutdown>
        </textarea>

        <buttonlist name="list">
            <area>425,180,430,470</area>
            <drawfrombottom>yes</drawfrombottom>
            <showarrow>no</showarrow>
            <layout>vertical</layout>
            <spacing>3</spacing>
            <wrapstyle>selection</wrapstyle>
            <buttonarea>0,0,100%,97%</buttonarea>
            <statetype name="buttonitem">
        <!--    ACTIVE  -->
                <state name="active">
                    <area>0,0,100%,31</area>
                    <shape name="shape_button_background">
                        <area>0,0,100%,100%</area>
                        <fill color="#000000" alpha="180" />
                    </shape>
                    <shape name="line_top_a" from="shape_line_a_horiz">
                        <area>0,-3,50%,2</area>
                    </shape>
                    <shape name="line_top_b" from="shape_line_b_horiz">
                        <area>50%,-3,100%,2</area>
                    </shape>
                    <shape name="line_bottom_a" from="shape_line_a_horiz">
                        <area>0,100%,50%,2</area>
                    </shape>
                    <shape name="line_bottom_b" from="shape_line_b_horiz">
                        <area>50%,100%,100%,2</area>
                    </shape>
                    <textarea name="buttontext">
                        <alpha>255</alpha>
                        <area>0,1,100%,30</area>
                        <font>normal</font>
                        <multiline>no</multiline>
                        <align>allcenter</align>
                    </textarea>
                    <!-- checkbox -->
                    <statetype name="buttoncheck">
                        <position>91%,5</position>
                        <state type="off">
                            <imagetype name="checkoff">
                                <filename>images/shared/check_none.png</filename>
                            </imagetype>
                        </state>
                        <state type="half">
                            <imagetype name="checkhalf">
                                <filename>images/shared/check_half.png</filename>
                            </imagetype>
                        </state>
                        <state type="full">
                            <imagetype name="checkfull">
                                <filename>images/shared/check_full.png</filename>
                            </imagetype>
                        </state>
                    </statetype>
                    <imagetype name="buttonarrow">
                        <position>96%,2</position>
                        <filename>images/shared/arrow_right.png</filename>
                    </imagetype>
                </state>
        <!--    SELECTED ACTIVE -->
                <state name="selectedactive" from="active">
                    <shape name="line_a">
                        <area>0,0,50%,100%</area>
                        <fill style="gradient">
                            <gradient start="#4d3f3f" end="#FFFFFF" alpha="140" direction="horizontal" />
                        </fill>
                    </shape>
                    <shape name="line_b">
                        <area>50%,0,100%,100%</area>
                        <fill style="gradient">
                            <gradient start="#FFFFFF" end="#4d3f3f" alpha="140" direction="horizontal" />
                        </fill>
                    </shape>
                </state>
            </statetype>
        <!--    ARROWS  -->
            <statetype name="upscrollarrow">
                <position>10,450</position>
                <state type="off">
                    <imagetype name="upon">
                        <filename>images/shared/arrow_up.png</filename>
                        <alpha>90</alpha>
                    </imagetype>
                </state>
                <state type="full">
                    <imagetype name="upoff">
                        <filename>images/shared/arrow_up.png</filename>
                    </imagetype>
                </state>
            </statetype>
            <statetype name="downscrollarrow">
                <position>400,450</position>
                <state type="off">
                    <imagetype name="dnon">
                        <filename>images/shared/arrow_down.png</filename>
                        <alpha>90</alpha>
                    </imagetype>
                </state>
                <state type="full">
                    <imagetype name="dnoff">
                        <filename>images/shared/arrow_down.png</filename>
                    </imagetype>
                </state>
            </statetype>
        </buttonlist>

