  <!-- General Font that is used in many screens (normal size)-->
  <font name="text" from="normal">
    <pixelsize>20</pixelsize>
  </font>
  <font name="text_inactive" from="text">
    <color>#999999</color>
  </font>
  <font name="text_error" from="text">
    <color>#800600</color>
  </font>
  <font name="text_warning" from="text">
    <color>#FFFF33</color>
  </font>

