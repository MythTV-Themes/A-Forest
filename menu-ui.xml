  <!-- ===================================================== -->
  <!--   THE MENU ITEMS                                      -->
  <!-- ===================================================== -->
    <shape name="menu_background_list">
      <area>160,-5,450,730</area>
<!--       <fill color="#131313" alpha="235" /> -->
      <fill color="#000000" alpha="200" />
       <line color="#AAAAAA" alpha="200" width="1" />
    </shape>
    <buttonlist name="menu">
      <area>160,20,450,680</area>
      <layout>vertical</layout>
      <spacing>15</spacing>
      <wrapstyle>items</wrapstyle>
      <buttonarea>0,30,450,660</buttonarea>
      <statetype name="buttonitem">
        <!-- 
             ACTIVE
        -->
        <state name="active">
          <area>0,0,450,56</area>
          <shape name="shape_menu_background_selecteditem"/>
          <textarea name="buttontext">
            <area>30,0,420,50</area>
            <font>menu</font>
            <align>left,vcenter</align>
            <cutdown>yes</cutdown>
          </textarea>
        </state>
        <!-- 
             SELECTED ACTIVE
        -->
        <state name="selectedactive" from="active">
          <shape name="shape_menu_background_selecteditem">
            <area>-20,0,490,75</area>
            <type>box</type>
            <fill color="#000000" alpha="200" />
            <line color="#AAAAAA" alpha="150" width="1" />
          </shape>
          <textarea name="buttontext">
            <font>menu_active</font>
          </textarea>
          <textarea name="description">
            <area>30,35,420,50</area>
            <font>menu_description</font>
            <align>left,vcenter</align>
            <cutdown>yes</cutdown>
          </textarea>
        </state>
      </statetype>

      <!-- ===================================================== -->
      <!--   ARROWS                                              -->
      <!-- ===================================================== -->
      <statetype name="upscrollarrow">
        <position>30,0</position>
        <showempty>yes</showempty>
        <state type="off">
          <imagetype name="upon">
            <filename>images/shared/arrow_up.png</filename>
            <alpha>50</alpha>
          </imagetype>
        </state>
        <state type="full">
          <imagetype name="upoff">
            <filename>images/shared/arrow_up.png</filename>
          </imagetype>
        </state>
      </statetype>
      <statetype name="downscrollarrow">
        <position>30,675</position>
        <showempty>yes</showempty>
        <state type="off">
          <imagetype name="dnon">
            <filename>images/shared/arrow_down.png</filename>
            <alpha>50</alpha>
          </imagetype>
        </state>
        <state type="full">
          <imagetype name="dnoff">
            <filename>images/shared/arrow_down.png</filename>
          </imagetype>
        </state>
      </statetype>
    </buttonlist>
